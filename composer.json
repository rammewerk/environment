{
  "name": "rammewerk/environment",
  "type": "library",
  "description": "A simple and fast environment variable handler for PHP projects.",
  "homepage": "https://rammewerk.com",
  "license": "MIT",
  "keywords": [
    "env",
    "dotenv",
    "environment"
  ],
  "authors": [
    {
      "name": "Kristoffer Follestad",
      "email": "kristoffer@rammewerk.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rammewerk\\Component\\Environment\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/",
      "/.idea"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rammewerk\\Component\\Environment\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1.0",
    "phpunit/phpunit": "^11.5"
  }
}